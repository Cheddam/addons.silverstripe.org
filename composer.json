{
  "name": "silverstripe/addons.silverstripe.org",
  "description": "The SilverStripe addons listing site",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/silverstripe/silverstripe-globaltoolbar.git"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "composer/composer": "^1.2",
    "ezyang/htmlpurifier": "4.5.*@stable",
    "silverstripe/framework": "3.1.*@stable",
    "silverstripe/multivaluefield": "*",
    "silverstripe/toolbar": "^4.0",
    "knplabs/packagist-api": "1.*@stable",
    "stojg/silverstripe-resque": "*",
    "silverstripe-australia/elastica": "dev-master",
    "symfony/event-dispatcher": "^2.8",
    "symfony/filesystem": "^2.8",
    "symfony/process": "^2.8",
    "symfony/finder": "^2.8",
    "symfony/console": "^2.8",
    "symfony/yaml": "^2.8"
  },
  "replace": {
    "extensions.silverstripe.org": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "silverstripe/sqlite3": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
